<%= simple_form_for @bug do |f| %>

	<%= f.input :name %>
	<%= f.input :description %>

	<%= f.input :url %>
	<%= f.input :browser %>
	<%= f.input :params %>
	<%= f.input :width %>
	<%= f.input :height %>
	<%= f.input :browser_version %>
	<%= f.input :os %>
	<%= f.input :ua %>

	<%= f.button :submit %>

<% end %>

