<!-- < %= render 'form', bug: @bug, url: dashboard_project_bugs_path %> -->

<%= simple_form_for @bug, url: dashboard_project_bugs_path, remote: true do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.association :admin, label: 'Currently assigned admin', include_blank: false  %>
  <%= f.input :time_spent %>
  
  <%= f.button :submit %>

<% end %>