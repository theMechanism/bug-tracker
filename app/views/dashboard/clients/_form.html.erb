<%= simple_form_for client, url: url, remote: true do |f| %>
  
  <%= f.input :name_of_co, label: 'Name of Company' %>
  <%= f.input :name_of_primary_contact %>
  <%= f.input :email %>
  <%= f.input :phone %>
  
  <% unless client.encrypted_password %>
    <%= f.input :password, label: 'Placeholder password for client' %>
  <% end %>
  
  <%= f.input :misc_info %>

  <%= f.button :submit %>

<% end %>

